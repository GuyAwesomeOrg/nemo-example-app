{
  "plugins": {
    "view": {
      "module": "nemo-view",
      "arguments": ["path:locator"]
    },
    "page": {
      "module": "nemo-page",
      "arguments": [
        "path:page-locator",
        null
      ]
    },
    "screenshot": {
      "module": "nemo-screenshot",
      "arguments": ["path:report", ["exception", "click"]]
    },
    "logger": {
      "module": "nemo-logger",
      "arguments": ["WARNING"]
    },
    "wdb": {
      "module": "nemo-wd-bridge"
    },
    "nemo-fixo": {
      "modules": "nemo-fixo",
      "arguments": [{
        "baseDir": "path:fixture"
      }]
    }
  },
  "driver": {
    "browser": "firefox"
  },
  "data": {
    "baseUrl": "https://fast-castle-8102.herokuapp.com"
  }
}
